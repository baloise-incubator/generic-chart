"":
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      labels:
        app: generic-chart
        helm.sh/chart: generic-chart-3.9.3
        release: simple-app
      name: generic-chart
      namespace: namespace-test
    spec:
      replicas: 1
      revisionHistoryLimit: 1
      selector:
        matchLabels:
          app: generic-chart
          release: simple-app
      strategy:
        type: RollingUpdate
      template:
        metadata:
          labels:
            app: generic-chart
            release: simple-app
        spec:
          affinity:
            podAntiAffinity:
              requiredDuringSchedulingIgnoredDuringExecution:
              - labelSelector:
                  matchExpressions:
                  - key: app
                    operator: In
                    values:
                    - generic-chart
                  - key: release
                    operator: In
                    values:
                    - simple-app
                topologyKey: kubernetes.io/hostname
          containers:
          - args:
            - -nginx.retry-interval=10s
            env:
            - name: SCRAPE_URI
              value: http://127.0.0.1:8080/stub_status
            image: quay.balgroupit.com/devops/nginx-prometheus-exporter:0.9.0
            name: nginx-exporter
            resources:
              limits:
                cpu: 100m
                memory: 32Mi
              requests:
                cpu: 10m
                memory: 16Mi
            volumeMounts:
            - mountPath: /tmp
              name: special-config
            - mountPath: /var/data
              name: data
          - image: registry.example.tld/org/repo:v1
            imagePullPolicy: IfNotPresent
            name: generic-chart
            ports:
            - containerPort: 8080
              name: http
              protocol: TCP
            resources: {}
            securityContext: {}
          initContainers:
          - command:
            - sh
            - -c
            - echo test
            env:
            - name: INIT_DEMO_VARIABLE
              value: Init Demo Variable
            image: quay.balgroupit.com/baloise-base-images/busybox:1.31.1
            name: demo-init-container
            volumeMounts:
            - mountPath: /tmp
              name: special-config
          securityContext: {}
          serviceAccountName: generic-chart
          topologySpreadConstraints:
          - labelSelector:
              matchLabels:
                app: generic-chart
                release: simple-app
            maxSkew: 1
            topologyKey: topology.kubernetes.io/any
            whenUnsatisfiable: DoNotSchedule
  2: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app: generic-chart
        helm.sh/chart: generic-chart-3.9.3
        release: simple-app
      name: generic-chart
      namespace: namespace-test
    spec:
      ports:
      - name: http
        port: 8080
        protocol: TCP
        targetPort: http
      selector:
        app: generic-chart
        release: simple-app
      type: ClusterIP
  3: |
    apiVersion: v1
    automountServiceAccountToken: false
    kind: ServiceAccount
    metadata:
      labels:
        app: generic-chart
        helm.sh/chart: generic-chart-3.9.3
        release: simple-app
      name: generic-chart
      namespace: namespace-test
